{
  "name": "@bloom-housing/public-reference",
  "version": "0.2.13",
  "description": "Public web app reference implementation for the Bloom affordable housing system",
  "main": "index.js",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next -p ${NEXTJS_PORT:-3000}",
    "build": "next build",
    "test": "concurrently \"yarn dev\" \"cypress open\"",
    "export": "next export",
    "start": "next start",
    "dev:listings": "cd ../../services/listings && yarn dev",
    "dev:server-wait": "wait-on \"http://localhost:${PORT:-3001}/\" && yarn dev",
    "dev:all": "concurrently \"yarn dev:listings\" \"yarn dev:server-wait\""
  },
  "dependencies": {
    "@bloom-housing/core": "^0.2.13",
    "@bloom-housing/ui-components": "^0.2.13",
    "@mdx-js/loader": "1.6.16",
    "@next/mdx": "9.5.1",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "moment": "^2.27.0",
    "nanoid": "^3.1.12",
    "next": "9.5.1",
    "next-plugin-custom-babel-config": "^1.0.2",
    "node-polyglot": "^2.4.0",
    "node-sass": "^4.14.1",
    "openapi-client-axios": "^3.6.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^6.3.0",
    "react-inlinesvg": "^2.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "9.5.1",
    "@types/node": "^12.12.50",
    "@types/react": "^16.9.44",
    "concurrently": "^5.2.0",
    "cypress": "^4.12.0",
    "js-levenshtein": "^1.1.6",
    "next-transpile-modules": "^4.0.2",
    "sass-loader": "^9.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1"
  }
}
