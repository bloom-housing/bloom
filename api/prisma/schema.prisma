// --- Meta Information --- //
generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["postgresqlExtensions", "views", "driverAdapters"]
}

datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
  extensions = [uuidOssp(map: "uuid-ossp")]
}

// --- Model Section --- //

// Accessibility Section
model Accessibility {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)
  updatedAt DateTime @updatedAt @map("updated_at") @db.Timestamp(6)

  hearing  Boolean?
  mobility Boolean?
  other    Boolean?
  vision   Boolean?

  applications Applications?

  @@map("accessibility")
}

model AccessibilitySnapshot {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)

  originalId        String   @map("original_id") @db.Uuid
  originalCreatedAt DateTime @map("original_created_at") @db.Timestamp(6)
  updatedAt         DateTime @map("updated_at") @db.Timestamp(6)

  hearing  Boolean?
  mobility Boolean?
  other    Boolean?
  vision   Boolean?

  application ApplicationSnapshot?

  @@map("accessibility_snapshot")
}

// AcivityLog Section
model ActivityLog {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)
  updatedAt DateTime @updatedAt @map("updated_at") @db.Timestamp(6)

  action   String  @db.VarChar
  metadata Json?
  module   String  @db.VarChar
  recordId String? @map("record_id") @db.Uuid
  userId   String? @map("user_id") @db.Uuid

  userAccounts UserAccounts? @relation(fields: [userId], references: [id], onUpdate: NoAction)

  @@map("activity_log")
}

// Address Section
model Address {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)
  updatedAt DateTime @updatedAt @map("updated_at") @db.Timestamp(6)

  city      String?
  county    String?
  latitude  Decimal? @db.Decimal
  longitude Decimal? @db.Decimal
  placeName String?  @map("place_name")
  state     String?
  street    String?
  street2   String?
  zipCode   String?  @map("zip_code")

  alternateContact             AlternateContact?
  applicantAddress             Applicant?                   @relation("applicant_address")
  applicantWorkAddress         Applicant?                   @relation("applicant_work_address")
  applicationDropOffAddress    Listings[]                   @relation("application_drop_off_address")
  applicationMailingAddress    Listings[]                   @relation("application_mailing_address")
  applicationPickUpAddress     Listings[]                   @relation("application_pick_up_address")
  applicationsAlternateAddress Applications?                @relation("applications_alternate_address")
  applicationSelectionAddress  ApplicationSelectionOptions? @relation("application_selection_address")
  applicationsMailingAddress   Applications?                @relation("applications_mailing_address")
  buildingAddress              Listings[]                   @relation("building_address")
  householdMemberAddress       HouseholdMember?             @relation("household_member_address")
  householdMemberWorkAddress   HouseholdMember?             @relation("household_member_work_address")
  leasingAgentAddress          Listings[]                   @relation("leasing_agent_address")
  userAccounts                 UserAccounts[]

  @@map("address")
}

model AddressSnapshot {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)

  originalId        String   @map("original_id") @db.Uuid
  originalCreatedAt DateTime @map("original_created_at") @db.Timestamp(6)
  updatedAt         DateTime @map("updated_at") @db.Timestamp(6)

  city      String?
  county    String?
  latitude  Decimal? @db.Decimal
  longitude Decimal? @db.Decimal
  placeName String?  @map("place_name")
  state     String?
  street    String?
  street2   String?
  zipCode   String?  @map("zip_code")

  alternateContact             AlternateContactSnapshot?
  applicantAddress             ApplicantSnapshot?                  @relation("applicant_address_snapshot")
  applicantWorkAddress         ApplicantSnapshot?                  @relation("applicant_work_address_snapshot")
  applicationDropOffAddress    ListingSnapshot[]                   @relation("application_drop_off_address_snapshot")
  applicationMailingAddress    ListingSnapshot[]                   @relation("application_mailing_address_snapshot")
  applicationPickUpAddress     ListingSnapshot[]                   @relation("application_pick_up_address_snapshot")
  applicationsAlternateAddress ApplicationSnapshot?                @relation("application_alternate_address_snapshot")
  applicationSelectionAddress  ApplicationSelectionOptionSnapshot? @relation("application_selection_address_snapshot")
  applicationsMailingAddress   ApplicationSnapshot?                @relation("application_mailing_address_snapshot")
  buildingAddress              ListingSnapshot[]                   @relation("building_address_snapshot")
  householdMemberAddress       HouseholdMemberSnapshot?            @relation("household_member_address_snapshot")
  householdMemberWorkAddress   HouseholdMemberSnapshot?            @relation("household_member_work_address_snapshot")
  leasingAgentAddress          ListingSnapshot[]                   @relation("leasing_agent_address_snapshot")
  userAccountSnapshots         UserAccountSnapshot[]

  @@map("address_snapshot")
}

// Agency Section
model Agency {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)
  updatedAt DateTime @updatedAt @map("updated_at") @db.Timestamp(6)

  jurisdictionsId String? @map("jurisdictions_id") @db.Uuid
  name            String

  jurisdictions        Jurisdictions?        @relation(fields: [jurisdictionsId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  userAccounts         UserAccounts[]
  userAccountSnapshots UserAccountSnapshot[]

  @@map("agency")
}

// AlternateContact Section
model AlternateContact {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)
  updatedAt DateTime @updatedAt @map("updated_at") @db.Timestamp(6)

  agency           String?
  emailAddress     String? @map("email_address")
  firstName        String? @map("first_name")
  lastName         String? @map("last_name")
  mailingAddressId String? @unique() @map("mailing_address_id") @db.Uuid
  otherType        String? @map("other_type")
  phoneNumber      String? @map("phone_number")
  type             String?

  address      Address?      @relation(fields: [mailingAddressId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  applications Applications?

  @@map("alternate_contact")
}

model AlternateContactSnapshot {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)

  originalId        String   @map("original_id") @db.Uuid
  originalCreatedAt DateTime @map("original_created_at") @db.Timestamp(6)
  updatedAt         DateTime @map("updated_at") @db.Timestamp(6)

  agency                   String?
  emailAddress             String? @map("email_address")
  firstName                String? @map("first_name")
  lastName                 String? @map("last_name")
  mailingAddressSnapshotId String? @unique() @map("mailing_address_snapshot_id") @db.Uuid
  otherType                String? @map("other_type")
  phoneNumber              String? @map("phone_number")
  type                     String?

  address     AddressSnapshot?     @relation(fields: [mailingAddressSnapshotId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  application ApplicationSnapshot?

  @@map("alternate_contact_snapshot_snapshot")
}

// AmiChart Section
model AmiChart {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)
  updatedAt DateTime @updatedAt @map("updated_at") @db.Timestamp(6)

  items          Json
  jurisdictionId String @map("jurisdiction_id") @db.Uuid
  name           String @db.VarChar

  jurisdictions              Jurisdictions               @relation(fields: [jurisdictionId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  unitGroupAmiLevels         UnitGroupAmiLevels[]
  units                      Units[]
  unitGroupAmiLevelSnapshots UnitGroupAmiLevelSnapshot[]
  unitSnapshots              UnitSnapshot[]

  @@map("ami_chart")
}

// Applicant Section
model Applicant {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)
  updatedAt DateTime @updatedAt @map("updated_at") @db.Timestamp(6)

  addressId       String?    @unique() @map("address_id") @db.Uuid
  birthDay        Int?       @map("birth_day")
  birthMonth      Int?       @map("birth_month")
  birthYear       Int?       @map("birth_year")
  emailAddress    String?    @map("email_address")
  firstName       String?    @map("first_name")
  fullTimeStudent YesNoEnum? @map("full_time_student")
  lastName        String?    @map("last_name")
  middleName      String?    @map("middle_name")
  noEmail         Boolean?   @map("no_email")
  noPhone         Boolean?   @map("no_phone")
  phoneNumber     String?    @map("phone_number")
  phoneNumberType String?    @map("phone_number_type")
  workAddressId   String?    @unique() @map("work_address_id") @db.Uuid
  workInRegion    YesNoEnum? @map("work_in_region")

  applicantAddress     Address?      @relation("applicant_address", fields: [addressId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  applicantWorkAddress Address?      @relation("applicant_work_address", fields: [workAddressId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  applications         Applications?

  @@map("applicant")
}

model ApplicantSnapshot {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)

  originalId        String   @map("original_id") @db.Uuid
  originalCreatedAt DateTime @map("original_created_at") @db.Timestamp(6)
  updatedAt         DateTime @map("updated_at") @db.Timestamp(6)

  addressSnapshotId     String?    @unique() @map("address_snapshot_id") @db.Uuid
  birthDay              Int?       @map("birth_day")
  birthMonth            Int?       @map("birth_month")
  birthYear             Int?       @map("birth_year")
  emailAddress          String?    @map("email_address")
  firstName             String?    @map("first_name")
  fullTimeStudent       YesNoEnum? @map("full_time_student")
  lastName              String?    @map("last_name")
  middleName            String?    @map("middle_name")
  noEmail               Boolean?   @map("no_email")
  noPhone               Boolean?   @map("no_phone")
  phoneNumber           String?    @map("phone_number")
  phoneNumberType       String?    @map("phone_number_type")
  workAddressSnapshotId String?    @unique() @map("work_address_snapshot_id") @db.Uuid
  workInRegion          YesNoEnum? @map("work_in_region")

  applicantAddress     AddressSnapshot?     @relation("applicant_address_snapshot", fields: [addressSnapshotId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  applicantWorkAddress AddressSnapshot?     @relation("applicant_work_address_snapshot", fields: [workAddressSnapshotId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  application          ApplicationSnapshot?

  @@map("applicant_snapshot")
}

// ApplicationFlaggedSet Section
model ApplicationFlaggedSet {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)
  updatedAt DateTime @updatedAt @map("updated_at") @db.Timestamp(6)

  listingId             String               @map("listing_id") @db.Uuid
  resolvedTime          DateTime?            @map("resolved_time") @db.Timestamptz(6)
  resolvingUserId       String?              @map("resolving_user_id") @db.Uuid
  rule                  RuleEnum
  ruleKey               String               @map("rule_key") @db.VarChar
  showConfirmationAlert Boolean              @default(false) @map("show_confirmation_alert")
  status                FlaggedSetStatusEnum @default(pending)

  applications Applications[]
  listings     Listings       @relation(fields: [listingId], references: [id], onDelete: Cascade, onUpdate: NoAction)
  userAccounts UserAccounts?  @relation(fields: [resolvingUserId], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@unique([ruleKey, listingId])
  @@index([listingId])
  @@map("application_flagged_set")
}

// ApplicationLotteryPosition Section
model ApplicationLotteryPositions {
  id String @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid

  applicationId         String  @map("application_id") @db.Uuid
  listingId             String  @map("listing_id") @db.Uuid
  multiselectQuestionId String? @map("multiselect_question_id") @db.Uuid
  ordinal               Int

  applications         Applications          @relation(fields: [applicationId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  listings             Listings              @relation(fields: [listingId], references: [id], onDelete: Cascade, onUpdate: NoAction)
  multiselectQuestions MultiselectQuestions? @relation(fields: [multiselectQuestionId], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([listingId])
  @@index([applicationId])
  @@map("application_lottery_positions")
}

// ApplicationLotteryTotal Section
model ApplicationLotteryTotal {
  id String @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid

  listingId             String  @map("listing_id") @db.Uuid
  multiselectQuestionId String? @map("multiselect_question_id") @db.Uuid
  total                 Int

  listings             Listings              @relation(fields: [listingId], references: [id], onDelete: Cascade, onUpdate: NoAction)
  multiselectQuestions MultiselectQuestions? @relation(fields: [multiselectQuestionId], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([listingId])
  @@map("application_lottery_totals")
}

// ApplicationMethod Section
model ApplicationMethods {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)
  updatedAt DateTime @updatedAt @map("updated_at") @db.Timestamp(6)

  acceptsPostmarkedApplications Boolean?                   @map("accepts_postmarked_applications")
  externalReference             String?                    @map("external_reference")
  label                         String?
  listingId                     String?                    @map("listing_id") @db.Uuid
  phoneNumber                   String?                    @map("phone_number")
  type                          ApplicationMethodsTypeEnum

  listings          Listings?           @relation(fields: [listingId], references: [id], onDelete: Cascade, onUpdate: NoAction)
  paperApplications PaperApplications[]

  @@map("application_methods")
}

model ApplicationMethodSnapshot {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)

  originalId        String   @map("original_id") @db.Uuid
  originalCreatedAt DateTime @map("original_created_at") @db.Timestamp(6)
  updatedAt         DateTime @map("updated_at") @db.Timestamp(6)

  acceptsPostmarkedApplications Boolean?                   @map("accepts_postmarked_applications")
  externalReference             String?                    @map("external_reference")
  label                         String?
  listingSnapshotId             String?                    @map("listing_snapshot_id") @db.Uuid
  phoneNumber                   String?                    @map("phone_number")
  type                          ApplicationMethodsTypeEnum

  listing          ListingSnapshot?           @relation(fields: [listingSnapshotId], references: [id], onDelete: Cascade, onUpdate: NoAction)
  paperApplication PaperApplicationSnapshot[]

  @@map("application_method_snapshot")
}

// Application Section
model Applications {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)
  updatedAt DateTime @updatedAt @map("updated_at") @db.Timestamp(6)

  acceptedTerms                  Boolean?                      @map("accepted_terms")
  accessibilityId                String?                       @unique() @map("accessibility_id") @db.Uuid
  accessibleUnitWaitlistNumber   Int?                          @map("accessible_unit_waitlist_number")
  additionalPhone                Boolean?                      @map("additional_phone")
  additionalPhoneNumber          String?                       @map("additional_phone_number")
  additionalPhoneNumberType      String?                       @map("additional_phone_number_type")
  alternateAddressId             String?                       @unique() @map("alternate_address_id") @db.Uuid
  alternateContactId             String?                       @unique() @map("alternate_contact_id") @db.Uuid
  applicantId                    String?                       @unique() @map("applicant_id") @db.Uuid
  appUrl                         String?                       @map("app_url")
  confirmationCode               String                        @map("confirmation_code")
  contactPreferences             String[]                      @map("contact_preferences")
  conventionalUnitWaitlistNumber Int?                          @map("conventional_unit_waitlist_number")
  deletedAt                      DateTime?                     @map("deleted_at") @db.Timestamp(6)
  demographicsId                 String?                       @unique() @map("demographics_id") @db.Uuid
  // Signifies when PII data should be cleared
  expireAfter                    DateTime?                     @map("expire_after") @db.Timestamp(6)
  householdExpectingChanges      Boolean?                      @map("household_expecting_changes")
  householdSize                  Int?                          @map("household_size")
  householdStudent               Boolean?                      @map("household_student")
  housingStatus                  String?                       @map("housing_status")
  income                         String?
  incomePeriod                   IncomePeriodEnum?             @map("income_period")
  incomeVouchers                 Boolean?                      @map("income_vouchers")
  // This application is the most recent for the user
  isNewest                       Boolean?                      @default(false) @map("is_newest")
  language                       LanguagesEnum?
  listingId                      String?                       @map("listing_id") @db.Uuid
  mailingAddressId               String?                       @unique() @map("mailing_address_id") @db.Uuid
  manualLotteryPositionNumber    Int?                          @map("manual_lottery_position_number")
  // if this field is true then the application is a confirmed duplicate 
  // meaning that the record in the application flagged set table has a status of duplicate
  markedAsDuplicate              Boolean                       @default(false) @map("marked_as_duplicate")
  preferences                    Json
  programs                       Json?
  reviewStatus                   ApplicationReviewStatusEnum   @default(pending) @map("review_status")
  sendMailToMailingAddress       Boolean?                      @map("send_mail_to_mailing_address")
  status                         ApplicationStatusEnum         @default(submitted)
  submissionDate                 DateTime?                     @map("submission_date") @db.Timestamptz(6)
  submissionType                 ApplicationSubmissionTypeEnum @map("submission_type")
  userId                         String?                       @map("user_id") @db.Uuid
  // Used to denote when an application was transferred from another jurisdiction
  wasCreatedExternally           Boolean                       @default(false) @map("was_created_externally")
  wasPIICleared                  Boolean?                      @default(false) @map("was_pii_cleared")

  accessibility                Accessibility?                @relation(fields: [accessibilityId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  alternateContact             AlternateContact?             @relation(fields: [alternateContactId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  applicant                    Applicant?                    @relation(fields: [applicantId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  applicationFlaggedSet        ApplicationFlaggedSet[]
  applicationLotteryPositions  ApplicationLotteryPositions[]
  applicationsAlternateAddress Address?                      @relation("applications_alternate_address", fields: [alternateAddressId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  applicationSelections        ApplicationSelections[]
  applicationsMailingAddress   Address?                      @relation("applications_mailing_address", fields: [mailingAddressId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  demographics                 Demographics?                 @relation(fields: [demographicsId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  householdMember              HouseholdMember[]
  listings                     Listings?                     @relation(fields: [listingId], references: [id], onDelete: SetNull, onUpdate: NoAction)
  preferredUnitTypes           UnitTypes[]
  userAccounts                 UserAccounts?                 @relation(fields: [userId], references: [id], onUpdate: NoAction)

  @@unique([listingId, confirmationCode])
  @@index([listingId])
  @@index([userId])
  @@index([isNewest])
  @@map("applications")
}

model ApplicationSnapshot {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)

  originalId        String   @map("original_id") @db.Uuid
  originalCreatedAt DateTime @map("original_created_at") @db.Timestamp(6)
  updatedAt         DateTime @map("updated_at") @db.Timestamp(6)

  acceptedTerms                  Boolean?                      @map("accepted_terms")
  accessibilitySnapshotId        String?                       @unique() @map("accessibility_snapshot_id") @db.Uuid
  accessibleUnitWaitlistNumber   Int?                          @map("accessible_unit_waitlist_number")
  additionalPhone                Boolean?                      @map("additional_phone")
  additionalPhoneNumber          String?                       @map("additional_phone_number")
  additionalPhoneNumberType      String?                       @map("additional_phone_number_type")
  alternateAddressSnapshotId     String?                       @unique() @map("alternate_address_snapshot_id") @db.Uuid
  alternateContactSnapshotId     String?                       @unique() @map("alternate_contact_snapshot_id") @db.Uuid
  applicantSnapshotId            String?                       @unique() @map("applicant_snapshot_id") @db.Uuid
  appUrl                         String?                       @map("app_url")
  confirmationCode               String                        @map("confirmation_code")
  contactPreferences             String[]                      @map("contact_preferences")
  conventionalUnitWaitlistNumber Int?                          @map("conventional_unit_waitlist_number")
  deletedAt                      DateTime?                     @map("deleted_at") @db.Timestamp(6)
  demographicSnapshotId          String?                       @unique() @map("demographic_snapshot_id") @db.Uuid
  // Signifies when PII data should be cleared
  expireAfter                    DateTime?                     @map("expire_after") @db.Timestamp(6)
  householdExpectingChanges      Boolean?                      @map("household_expecting_changes")
  householdSize                  Int?                          @map("household_size")
  householdStudent               Boolean?                      @map("household_student")
  housingStatus                  String?                       @map("housing_status")
  income                         String?
  incomePeriod                   IncomePeriodEnum?             @map("income_period")
  incomeVouchers                 Boolean?                      @map("income_vouchers")
  // This application is the most recent for the user
  isNewest                       Boolean?                      @default(false) @map("is_newest")
  language                       LanguagesEnum?
  listingId                      String?                       @map("listing_id") @db.Uuid
  mailingAddressSnapshotId       String?                       @unique() @map("mailing_address_snapshot_id") @db.Uuid
  manualLotteryPositionNumber    Int?                          @map("manual_lottery_position_number")
  // if this field is true then the application is a confirmed duplicate 
  // meaning that the record in the application flagged set table has a status of duplicate
  markedAsDuplicate              Boolean                       @default(false) @map("marked_as_duplicate")
  preferences                    Json
  programs                       Json?
  reviewStatus                   ApplicationReviewStatusEnum   @default(pending) @map("review_status")
  sendMailToMailingAddress       Boolean?                      @map("send_mail_to_mailing_address")
  status                         ApplicationStatusEnum         @default(submitted)
  submissionDate                 DateTime?                     @map("submission_date") @db.Timestamptz(6)
  submissionType                 ApplicationSubmissionTypeEnum @map("submission_type")
  userId                         String?                       @map("user_id") @db.Uuid
  // Used to denote when an application was transferred from another jurisdiction
  wasCreatedExternally           Boolean                       @default(false) @map("was_created_externally")
  wasPIICleared                  Boolean?                      @default(false) @map("was_pii_cleared")

  accessibility                AccessibilitySnapshot?         @relation(fields: [accessibilitySnapshotId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  alternateContact             AlternateContactSnapshot?      @relation(fields: [alternateContactSnapshotId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  applicant                    ApplicantSnapshot?             @relation(fields: [applicantSnapshotId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  applicationsAlternateAddress AddressSnapshot?               @relation("application_alternate_address_snapshot", fields: [alternateAddressSnapshotId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  applicationSelections        ApplicationSelectionSnapshot[]
  applicationsMailingAddress   AddressSnapshot?               @relation("application_mailing_address_snapshot", fields: [mailingAddressSnapshotId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  demographic                  DemographicSnapshot?           @relation(fields: [demographicSnapshotId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  householdMember              HouseholdMemberSnapshot[]
  preferredUnitTypes           UnitTypes[]

  @@unique([listingId, confirmationCode])
  @@index([listingId])
  @@index([userId])
  @@index([isNewest])
  @@map("application_snapshot")
}

// ApplicationSelectionOption Section
model ApplicationSelectionOptions {
  id        String   @id() @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(6)
  updatedAt DateTime @updatedAt @map("updated_at") @db.Timestamp(6)

  addressHolderAddressId    String?  @unique() @map("address_holder_address_id") @db.Uuid
  addressHolderName         String?  @map("address_holder_name")
  addressHolderRelationship String?  @map("address_holder_relationship")
  applicationSelectionId    String   @map("application_selection_id") @db.Uuid
  isGeocodingVerified       Boolean? @map("is_geocoding_verified")
  multiselectOptionId       String   @map("multiselect_option_id") @db.Uuid

  addressHolderAddress Address?              @relation("application_selection_address", fields: [addressHolderAddressId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  applicationSelection ApplicationSelections @relation(fields: [applicationSelectionId], references: [id], onDelete: Cascade, onUpdate: NoAction)
  multiselectOption    MultiselectOptions    @relation(fields: [multiselectOptionId], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@map("application_selection_options")
